<resources>

<properties>
    <property id="appVersion" type="string">1.0.3</property>
    <property id="username" type="string"></property>
    <property id="password" type="string"></property>
</properties>

<settings>

    <setting propertyKey="@Properties.appVersion" title="@Strings.AppVersionTitle">
        <settingConfig type="alphaNumeric" readonly="true" />
    </setting>

    <setting propertyKey="@Properties.username" title="@Strings.ikUsername">
        <settingConfig type="alphaNumeric" required="true" />
    </setting>
    
    <setting propertyKey="@Properties.password" title="@Strings.ikPassword">
        <settingConfig type="password" required="true" />
    </setting>

</settings>

</resources>